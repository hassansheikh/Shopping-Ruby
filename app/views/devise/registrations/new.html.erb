<div class="content">
        <!--login-->
      <div class="login">
    <div class="main-agileits">
        <div class="form-w3agile form1">
          <h3>Register</h3>


          <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
            <%= devise_error_messages! %>

            <div class="key">
              <i class="fa fa-envelope" aria-hidden="true"></i>
              <%= f.text_field :email, autofocus: true, autocomplete: "email" ,placeholder: "Email"%>
              <div class="clearfix"></div>
            </div>

              <% if @minimum_password_length %>
              <em>(<%= @minimum_password_length %> characters minimum)</em>
              <% end %><br />
            <div class="key">
              <i class="fa fa-lock" aria-hidden="true"></i>
              <%= f.password_field :password, autocomplete: "new-password" ,placeholder: "Password"%>
              <div class="clearfix"></div>
            </div>
            <div class="key">
              <i class="fa fa-lock" aria-hidden="true"></i>
              <%= f.password_field :password_confirmation, autocomplete: "new-password" ,placeholder: "Confirm Password"%>
              <div class="clearfix"></div>
            </div>

            <div class="actions">
              <%= f.submit "Sign up" %>
            </div>
          <% end %>
        </div>
        <div class="forg">
            <%= link_to "Log in", new_session_path(resource_name) %><br />
          <div class="clearfix"></div>
          </div>
      </div>
    </div>
        <!--login-->
    </div>

